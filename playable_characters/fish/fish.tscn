script/source = "extends CharacterBody2D

@onready var animated_sprite_2d: AnimatedSprite2D = $AnimatedSprite2D
const SPEED = 500
const JUMP_VELOCITY = -500
const FLOATING = -500
@export_range(0,1) var acceleration = 0.3
@export_range(0,1) var deceleration = 0.3

func _physics_process(delta: float) -> void:
	# Add the gravity.
	if not is_on_floor():
		velocity += get_gravity() * delta * 0.25

	# Handle jump.
	if Input.is_action_just_pressed(\"jump\") and is_on_floor():
		velocity.y = JUMP_VELOCITY
	if Input.is_action_pressed(\"jump\") and not is_on_floor() and velocity.y > 0:
		velocity.y = move_toward(velocity.y, FLOATING, 25000 * delta)
	# Get the input direction and handle the movement/deceleration.
	# As good practice, you should replace UI actions with custom gameplay actions.
	var direction := Input.get_axis(\"left\", \"right\")
	if direction:
		velocity.x = move_toward( velocity.x, direction * SPEED, SPEED *acceleration)
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED*deceleration)
		
	if direction > 0:
		animated_sprite_2d.flip_h = false
	elif direction < 0:
		animated_sprite_2d.flip_h = true

	move_and_slide()
"

